#Segment tree
arr=[1,2,3,4,5,6,7,8,9]
segment_tree=[[-1,-1,-1]]*(2*len(arr)-1)
index_map=[-1]*len(arr)
void create_tree(start,end,tree_starting_index):
	global arr,segment_tree,index_map
	if start==end:
		segment_tree[][0]=start
		segment_tree[][1]=start
		segment_tree[][2]=arr[start]
		index_map[start]=

if __name__=="__main__":
	create_tree(0,len(arr)-1,1)
